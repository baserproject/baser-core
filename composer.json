{
    "name": "baserproject/baser-core",
    "description": "baserCMS plugin for CakePHP",
    "homepage": "https://basercms.net",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.1",
        "cakephp/authentication": "^2.3",
        "baserproject/bc-admin-third": "0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "BaserCore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BaserCore\\Test\\": "tests/"
        }
    }
}
